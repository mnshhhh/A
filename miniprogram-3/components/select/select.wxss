.select_all_view{
  display: flex;
  z-index: 90;
  width: 350rpx;
 }
 .select_view{
  text-align:left !important;
  display: inline-block;
  width: 350rpx!important;
 }
 .select_title{
  margin-right: 10rpx;
 }
 .inputPlaceholder
 {
  text-align:left !important;
  border: 1rpx solid rgb(211, 215, 218);
  background-color: rgb(255, 255, 255);
  padding-left: 20rpx;
  line-height: 80rpx;
  height: 100%;
  width: 335rpx!important;
  border-radius: 60rpx;
  position: relative;
  overflow:hidden;
  text-overflow: ellipsis;
  white-space:nowrap;
  color: rgb(134, 134, 134);
}
 .jiantou1{
  position: absolute;
  right: 10rpx;
  top: 8rpx;
 }
 .content{
  position: absolute;
  z-index: 90;
  min-width: 300rpx;
  max-height:0rpx;
  background: #FFFFFF;
  box-shadow: 0px 0px 10px 1px rgba(88, 88, 88, 0.8);
  border-radius: 0 0 5% 5%;
  padding: 20rpx;
  overflow-x: hidden;
  overflow-y: scroll;
  opacity: 0;
  transition: all 0.8s;
  border-radius: 20rpx;
 }
 .content.inputPlaceholder{
  padding: 10rpx 0;
 }
 .content view{
   height: 100rpx;
 }
 .select_view .active{
  color:#46678d;
 }
 .visiable{
   max-height:280rpx;
   opacity: 1;
 }